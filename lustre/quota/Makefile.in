MODULES := lquota

qmt-objs := \
	qmt_dev.o \
	qmt_entry.o \
	qmt_handler.o \
	qmt_lqa.o \
	qmt_lock.o \
	qmt_pool.o

qsd-objs := \
	qsd_config.o \
	qsd_entry.o \
	qsd_handler.o \
	qsd_lib.o \
	qsd_lock.o \
	qsd_reint.o \
	qsd_request.o \
	qsd_writeback.o

quota-objs := \
	lproc_quota.o \
	lquota_disk.o \
	lquota_entry.o \
	lquota_lib.o

lquota-objs := \
	$(qsd-objs) \
	$(qmt-objs) \
	$(quota-objs)

EXTRA_DIST := $(lquota-objs:%.o=%.c)
EXTRA_DIST += \
    lquota_internal.h \
    qmt_internal.h \
    qsd_internal.h

@INCLUDE_RULES@
