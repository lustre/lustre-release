KERNEL_SRPM=kernel-${lnxmaj}-${lnxrel}.src.rpm
SERIES=2.6-rhel5.series
EXTRA_VERSION=${lnxrel}_lustre.@VERSION@
LUSTRE_VERSION=@VERSION@

OFED_VERSION=inkernel

BASE_ARCHS="i686 x86_64 ia64 ppc64"
